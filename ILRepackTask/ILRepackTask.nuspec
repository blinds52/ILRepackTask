<?xml version="1.0"?>
<package>
  <metadata>
    <id>ILRepackTask</id>
    <version>1.0.3</version>
    <title>ILRepack Task</title>
    <authors>Eric Schultz</authors>
    <owners>Eric Schultz</owners>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2013</copyright>
    <licenseUrl>http://opensource.org/licenses/LGPL-3.0</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An MSBuild task and target for running ILRepack as part of a build. By default, the RepackILFiles target will repack the output assembly with all the other referenced assemblies copied into the output directory. The RepackILFiles target can be overridden.</description>
  </metadata>
  
  <files>
    <file src="bin\$configuration$\ILRepack.exe" target="build\net40\ILRepack.exe" />
    <file src="bin\$configuration$\ILRepackTask.dll" target="build\net40\ILRepackTask.dll" />
    <file src="bin\$configuration$\ILRepackTask.targets" target="build\net40\ILRepackTask.targets" />
    <file src="bin\$configuration$\System.IO.Abstractions.dll" target="build\net40\System.IO.Abstractions.dll" />
  </files>
</package>